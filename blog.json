        {
          "_key": "125873db1c1b0",
          "_type": "span",
          "marks": [],
          "text": "\n"
        },
        {
          "_key": "125873db1c1b1",
          "_type": "span",
          "marks": ["strong"],
          "text": "Step 2: Diving into Due Diligence"
        }
